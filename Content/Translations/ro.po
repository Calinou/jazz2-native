msgstr ""
"\n"
"Podul acela nu\n"
"nu pare prea sigur..."

#: .fake/Translations/xmas99/03_xmas3.j2l.h:5
msgctxt "xmas99/03_xmas3"
msgid ""
"\n"
"Robert and Craig:\n"
"Springs RULE! :)"
msgstr ""
"\n"
"Robert și Craig:\n"
"Arcurile RULESC! :)"

