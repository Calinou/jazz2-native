msgctxt "xmas99/03_xmas3"
msgid ""
"\n"
"Now leaving Burrowsville\n"
"Please visit again."
msgstr ""
"\n"
"Acum părăsiți \"Burrowsville\"\n"
"Vă rugăm să ne vizitați din nou."

#: .fake/Translations/xmas99/03_xmas3.j2l.h:7
msgctxt "xmas99/03_xmas3"
msgid ""
"\n"
"Password: xmasbunny\n"
"Please visit\n"
"www.project2.com"
msgstr ""
"\n"
"Parolă: xmasbunny\n"
"Vă rugăm să vizitați\n"
"www.project2.com"

